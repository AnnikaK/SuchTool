(defun split(expr-list)
  (let ((keywords-list '())(free-text '()))
    (mapcar (lambda (x) 
              (if (listp x) (setq keywords-list (append (list x) keywords-list))
                (setq free-text (append (list x) free-text))))
            expr-list)
 (list keywords-list free-text)))


;TODO: kommas und sonderzeichen abfangen
(defun readFile (file-path)
  (do* ((streamin (open file-path)) 
        exprs
        (expr (read streamin nil 'eof) 
              (read streamin nil 'eof)))
       ((equal expr 'eof) (close streamin)
        (nreverse exprs))
    (setq exprs (cons expr exprs))))




(split (readFile "C:\\Users\\kreme\\OneDrive\\PractisingLisp\\Lisp\\mail-beispiele-eingang\\mail-beispiele-eingang\\mail-588.ldd"))



    